{
  "name": "@percy/storybook",
  "version": "4.0.0-beta.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/percy/percy-storybook.git"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "main": "dist/index.js",
  "files": [
    "./dist",
    "./oclif.manifest.json"
  ],
  "scripts": {
    "build": "babel src --out-dir dist",
    "clean": "rm -rf dist .nyc_output coverage oclif.manifest.json",
    "lint": "eslint --ignore-path .gitignore .",
    "postbuild": "oclif-dev manifest",
    "readme": "oclif-dev readme",
    "test": "cross-env NODE_ENV=test mocha",
    "test:coverage": "nyc yarn test"
  },
  "oclif": {
    "bin": "percy",
    "commands": "./dist/commands"
  },
  "dependencies": {
    "@percy/cli-command": "^1.0.0-beta.31",
    "@percy/core": "^1.0.0-beta.31",
    "@percy/logger": "^1.0.0-beta.31",
    "cors": "^2.8.5",
    "cross-spawn": "^7.0.3",
    "express": "^4.17.1",
    "qs": "^6.9.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/register": "^7.8.3",
    "@oclif/dev-cli": "^1.22.2",
    "babel-eslint": "^10.0.3",
    "babel-plugin-istanbul": "^6.0.0",
    "cross-env": "^7.0.1",
    "eslint": "^7.2.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "expect": "^26.0.1",
    "mocha": "^8.0.1",
    "nyc": "^15.0.0"
  }
}
