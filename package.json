{
  "name": "@percy/storybook",
  "version": "3.3.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/percy/percy-storybook.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=10.0.0"
  },
  "bin": {
    "percy-storybook": "./bin/percy-storybook.js"
  },
  "main": "lib/index.js",
  "files": [
    "bin",
    "lib"
  ],
  "scripts": {
    "lint": "eslint --ext .js src/",
    "build": "babel src -d lib --ignore 'src/__tests__'",
    "prebuild": "rimraf ./lib",
    "prepublishOnly": "yarn build",
    "test:unit": "jest"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^10.0.0",
    "babel-jest": "^24.8.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "chalk": "^4.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jasmine": "*",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "fs-extra": "^7.0.0",
    "husky": "^4.2.5",
    "jest": "^23.0.0",
    "lint-staged": "^10.2.13",
    "mock-fs": "^4.12.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.0"
  },
  "dependencies": {
    "@percy/react-percy-api-client": "^0.4.6",
    "babel-runtime": "^6.26.0",
    "debug": "^3.1.0",
    "es6-error": "^4.0.2",
    "es6-promise-pool": "^2.4.4",
    "puppeteer": "^1.4.0",
    "walk": "^2.3.9",
    "yargs": "^7.0.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "globals": {
      "window": true
    }
  },
  "jest": {
    "testMatch": [
      "**/tests/unit/**-tests.js"
    ],
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
