{
  "name": "@percy/storybook",
  "version": "4.0.0-beta.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/percy/percy-storybook.git"
  },
  "main": "dist/index.js",
  "files": [
    "bin",
    "dist",
    "oclif.manifest.json"
  ],
  "bin": {
    "percy-storybook": "bin/percy-storybook"
  },
  "scripts": {
    "build": "babel src --out-dir dist",
    "clean": "git clean -Xdf --exclude !node_modules",
    "lint": "eslint --ignore-path .gitignore .",
    "postbuild": "oclif-dev manifest",
    "readme": "oclif-dev readme",
    "pretest": "build-storybook --config-dir=test/.storybook --output-dir=test/.storybook-build --loglevel error",
    "test": "cross-env NODE_ENV=test jasmine --config=test/jasmine.json",
    "test:coverage": "nyc yarn test"
  },
  "oclif": {
    "bin": "percy",
    "commands": "./dist/commands",
    "hooks": {
      "init": "./dist/hooks/init"
    }
  },
  "dependencies": {
    "@percy/cli-command": "^1.0.0-beta.52",
    "@percy/core": "^1.0.0-beta.52",
    "@percy/logger": "^1.0.0-beta.52",
    "cross-spawn": "^7.0.3",
    "qs": "^6.10.1",
    "serve-handler": "^6.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/eslint-parser": "^7.14.3",
    "@babel/eslint-plugin": "^7.13.6",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.14.2",
    "@babel/register": "^7.13.14",
    "@oclif/dev-cli": "^1.26.0",
    "@storybook/react": "^6.2.9",
    "babel-eslint": "^10.0.3",
    "babel-plugin-istanbul": "^6.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.27.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "jasmine": "^3.7.0",
    "jasmine-spec-reporter": "^7.0.0",
    "mock-require": "^3.0.3",
    "nock": "^13.0.11",
    "nyc": "^15.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
